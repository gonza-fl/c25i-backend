<!DOCTYPE html>
<html lang="es">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title><%= title%></title>
    <%- include('partials/head') %>
  </head>

  <body>
    <%- include('partials/navigation') %>
    <main class="container d-flex flex-column align-items-center w-100 vh-100">
      <h1>Contact</h1>
      <div class="car-body p-2">
        <form class="">
          <div class="email d-flex flex-column p-2">
            <label for="email" class="mb-1 form-label"> Email </label>
            <input name="email" type="email" placeholder="your@email.com" class="form-control" />
          </div>
          <div class="message d-flex flex-column p-2">
            <label for="message" class="mb-1 form-label"> Message </label>
            <textarea name="message" placeholder="Enter your message for me..." class="form-control"></textarea>
          </div>
        </form>
      </div>
    </main>
    <%- include('partials/footer') %> <%- include('partials/body_config') %>
  </body>
</html>
